        "squizlabs/php_codesniffer": "^2.6",
        "phpdocumentor/reflection-docblock": "2.*"
    },
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Steve Pfisterer",
            "email": "steve@stoutlogic.com"
        }
    ],
    "autoload": {
